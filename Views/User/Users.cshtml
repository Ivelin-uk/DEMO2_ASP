@model IEnumerable<MyMvcApp.Models.User>

<h1>Списък с потребители</h1>

<table class="table">
    <thead>
        <tr>
            <th>Име</th>
            <th>Имейл</th>
            <th>Действия</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName @user.LastName</td> 
                <td>@user.Email</td>
                <td>
                    <a href="#" class="btn btn-primary">Редактирай</a>
                    <a href="#" class="btn btn-danger">Изтрий</a>
                </td>
            </tr>
        }
    </tbody>
</table>